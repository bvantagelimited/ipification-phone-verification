
extends layout

block content
	.container
		.row
			.col-sm-12
				#login
					.app-logo
						img(src=`${ROOT_URL}/images/landing.svg`)

					.app-name
						.short-title Register with your
						.title Internet Banking Application

		.btn-block
			div.phone-title Phone
			.phone-input
				.phone-valid
				input.form-control(type='text')
			button.btn-login(type='button') Continue


			




